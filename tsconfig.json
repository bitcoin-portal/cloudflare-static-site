{
    "compilerOptions": {
      "module": "commonjs",
      "target": "esnext",
      "lib": ["es2015", "webworker"],
      "types": ["@cloudflare/workers-types"],
      "jsx": "react",
      "noImplicitAny": true,
      "preserveConstEnums": true,
      "outDir": "./dist",
      "moduleResolution": "node"
    },
    "include": ["src/*.ts", "src/**/*.ts", "src/*.tsx", "src/**/*.tsx"]
}